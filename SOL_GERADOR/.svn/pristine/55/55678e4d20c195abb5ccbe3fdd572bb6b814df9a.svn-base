     SQL.Add('   SELECT DISTINCT   ');
     SQL.Add('       PRODUTO.CODIGO AS COD_PRODUTO,   ');
     SQL.Add('       PRODUTO.PRVENDA_AVISTA AS VAL_VENDA   ');
     SQL.Add('   FROM   ');
