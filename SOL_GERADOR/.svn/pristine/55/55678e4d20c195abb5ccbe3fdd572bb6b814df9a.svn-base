procedure TFrmSmLaranjeiras.GerarNFFornec;
var
   TotalCount : integer;
begin
  inherited;
  with QryPrincipal do
  begin
    Close;
    SQL.Clear;

     SQL.Add('   SELECT DISTINCT   ');
     SQL.Add('       ENTRADA.ID_FORNECEDOR AS COD_FORNECEDOR,   ');
     SQL.Add('       ENTRADA.NF_NUMERO AS NUM_NF_FORN,   ');
     SQL.Add('       ENTRADA.NF_SERIE AS NUM_SERIE_NF,   ');
     SQL.Add('       '''' AS NUM_SUBSERIE_NF,   ');
     SQL.Add('       COALESCE(ENTRADA.CFOP, '''') AS CFOP,   ');
     SQL.Add('       0 AS TIPO_NF,   ');
     SQL.Add('       ''NFE'' AS DES_ESPECIE,   ');
     SQL.Add('       ENTRADA.TOT_NF AS VAL_TOTAL_NF,   ');
     SQL.Add('       ENTRADA.DT_EMISSAO AS DTA_EMISSAO,   ');
     SQL.Add('       ENTRADA.DT_ENTRADA AS DTA_ENTRADA,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_IPI, 0) AS VAL_TOTAL_IPI,   ');
     SQL.Add('       0 AS VAL_VENDA_VAREJO,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_FRETE, 0) AS VAL_FRETE,   ');
     SQL.Add('       0 AS VAL_ACRESCIMO,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_DESCONTO, 0) AS VAL_DESCONTO,   ');
     SQL.Add('       COALESCE(REPLACE(REPLACE(REPLACE(TB_FORNECEDOR.CNPJ, ''-'', ''''), ''.'', ''''), ''/'', ''''), '''') AS NUM_CGC,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_BC_ICMS, 0) AS VAL_TOTAL_BC,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_ICMS, 0) AS VAL_TOTAL_ICMS,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_BC_ST, 0) AS VAL_BC_SUBST,   ');
     SQL.Add('       COALESCE(ENTRADA.TOT_ST, 0) AS VAL_ICMS_SUBST,   ');
     SQL.Add('       0 AS VAL_FUNRURAL,   ');
     SQL.Add('       1 AS COD_PERFIL,   ');
     SQL.Add('       0 AS VAL_DESP_ACESS,   ');
     SQL.Add('       ''N'' AS FLG_CANCELADO,   ');
     SQL.Add('       '''' AS DES_OBSERVACAO,   ');
     SQL.Add('       COALESCE(ENTRADA.NFE_ORIGEM, '''') AS NUM_CHAVE_ACESSO   ');
     SQL.Add('   FROM   ');
     SQL.Add('       V_NFC AS ENTRADA   ');
     SQL.Add('   LEFT JOIN TB_FORNECEDOR ON TB_FORNECEDOR.ID_FORNEC = ENTRADA.ID_FORNECEDOR   ');
